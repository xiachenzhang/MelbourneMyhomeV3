    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script|Merienda" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>

    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href='https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css' rel='stylesheet' />

</head>
<body style="overflow-x:hidden;">
    <nav class="navbar navbar-default navbar-inverse" style="font-family: 'Merienda', cursive;background-color:#333">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Melbourne Myhome</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img src="~/Content/title.png" style="margin-top:2vh;margin-left:5vh" />
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav " style="margin-top:6px;margin-left:64px">
                    <li><a href="~/Home/Home"><img src="~/Content/homeicon.png" />Home <span class="sr-only">(current)</span></a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle;" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"> <img src="~/Content/learningicon.png" />Learning <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Pop/LearningCenter"><i class="fa fa-book" aria-hidden="true"></i>Learning Center</a></li>
                            <li><a href="~/Pop/PopOverview"><i class="fa fa-user" aria-hidden="true"></i>Population</a></li>
                            <li><a href="~/Culture/CulOverview"><i class="fa fa-globe" aria-hidden="true"></i>Culture</a></li>

                            <li><a href="~/Pop/Environment"><i class="fa fa-tree" aria-hidden="true"></i>Environment</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/Content/quizicon.png" />Quiz <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Quiz/QuizCenter"><i class="fa fa-file-text-o" aria-hidden="true"></i>Quiz Center</a></li>
                            <li><a href="~/Quiz/QuizPop"><i class="fa fa-female" aria-hidden="true"></i>Population Quiz</a></li>
                            <li><a href="~/Quiz/QuizCul"><i class="fa fa-flag" aria-hidden="true"></i>Culture Quiz</a></li>

                            <li><a href="~/Quiz/QuizEnv"><i class="fa fa-bolt" aria-hidden="true"></i>Environment Quiz</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/Content/gameicon.png" />Game <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Game/GameCenter"><i class="fa fa-gamepad" aria-hidden="true"></i>Game Center</a></li>
                            <li><a href="~/Game/MemoryGame"><i class="fa fa-github-alt" aria-hidden="true"></i>Memory Game</a></li>
                            <li><a href="~/Game/Tangram"><i class="fa fa-hand-o-up" aria-hidden="true"></i>Tangram Game</a></li>
                            <li><a href="~/Game/Capital"><i class="fa fa-check-square-o" aria-hidden="true"></i>Answer Game</a></li>
                            <li><a href="~/Game/RecycleGame"><i class="fa fa-recycle" aria-hidden="true"></i>Recycle Game</a></li>
                        </ul>
                    </li>

                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>

    <center><h1 style="font-size:36px;font-family: 'Merienda', cursive;font-size:3.5vw">Learning Center</h1></center>
    <div class="full-wrap" style="margin-top:5vh">
        <div class="menuwrap">
            <div class="menu">
                <div class="set" id="set1">
                    <div class="head">Learning</div>
                    <div class="desc">Overview</div>
                    
                    <div class="icon-star ico"></div>
                </div>
                <div class="set" id="set2">
                    <div data-content="Serenity" class="head">Learning</div>
                    <div class="desc">Population</div>
                    <div class="icon-heart ico"></div>
                </div>
                <div class="set" id="set3">
                    <div class="head">Learning</div>
                    <div class="desc">Culture</div>
                    <div class="icon-smile ico"></div>
                </div>
                <div class="set" id="set4">
                    <div class="head">Learning</div>
                    <div class="desc">Environment</div>
                    
                    <div class="icon-globe ico"></div>
                </div>
            </div>
        </div>

        <div class="content">
            <span><i class="icon-remove"></i> Close</span>
            <div id="page1" class="page">
                <h1 style="font-family: 'Kaushan', cursive;font-size:2.5vw">Overview</h1>
                <h1 style="font-family: 'Merienda', cursive;font-size:1.2vw">
                    We will tell the story of how Melbourne has changed over the years
                    in population, culture, and environment.
                    <br />
                    There are lot of interesting chart,images,we even prepared a story book for you,do you want
                    to have a look？
                </h1>
                <img src="~/Content/Learning1.png" style="width:50%;height:50%"/>

            </div>
            <div id="page2" class="page">
                <h1 style="font-family: 'Kaushan', cursive;font-size:2.5vw">Population</h1>
                <h1 style="font-family: 'Merienda', cursive;font-size:1.2vw">
                   
                    How many children in your age in Melbourne?<br />
                    Which suburb has the most population?<br /> How the population changed in Melbourne?<br />
                    … By reading through this module, you will know the answers.
                </h1>
                <img src="~/Content/family.png" style="width:50%;height:50%"/>
                <a href="~/Pop/PopOverview">
                    <img src="~/Content/arrow1.png" style="width:20%;height:20%"/>
                </a>
            </div>
            <div id="page3" class="page">
                <h1 style="font-family: 'Kaushan', cursive;font-size:2.5vw">Culture</h1>
                <h1 style="font-family: 'Merienda', cursive;font-size:1.2vw">
                    Do you want to know which countries your friends are from?<br />
                    Do you want to know the stories and lifestyles in which you live?<br />
                    Do you want to know the different languages in the world?<br />
                    Food, sights, interesting animals, you can find a lot of new things here. Start exploring now.
                </h1>
                <img src="~/Content/thanksgiving.png" style="width:50%;height:50%"/>
                <a href="~/Culture/CulOverview">
                    <img src="~/Content/arrow1.png" style="width:20%;height:20%"/>
                </a>
            </div>
            <div id="page4" class="page">
                <h1 style="font-family: 'Kaushan', cursive;font-size:2.5vw">Environment</h1>
                <h1 style="font-family: 'Merienda', cursive;font-size:1.2vw">
                    Do you know what is Environment?<br />
                    Do you know how to protect it?<br />

                    You can find some interesting vedio and <br />see some related information 
                    about tree,animal,bird here.
                </h1>

                <img src="~/Content/Tree111.png" style="width:50%;height:50%"/>
                <a href="~/Pop/Environment">
                    <img src="~/Content/arrow1.png" style="width:20%;height:20%"/>
                </a>
            </div>
        </div>

        <div class="info">Click on any option</div>

    </div>
    <script src="~/Scripts/Center.js"></script>


</body>