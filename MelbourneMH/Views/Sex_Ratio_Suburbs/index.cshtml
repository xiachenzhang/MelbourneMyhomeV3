@model IEnumerable<MelbourneMH.Models.Sex_Ratio_Suburbs>
@{
    ViewBag.Title = "PopLearning";
    Layout = null;

}
<head>

    <link href="~/Content/bubble.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Abel|Lobster" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <link href="~/Content/NARVIGATION.css" rel="stylesheet" />
    <meta name="viewport" content="initial-scale=1.0">
    <link href='https://fonts.googleapis.com/css?family=Economica' rel='stylesheet' type='text/css'>
    <script type="text/javascript" src="js/jq.js"></script>
    <script type="text/javascript" src="js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:200" rel="stylesheet">
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Hammersmith+One" rel="stylesheet">
    <link href="~/Content/LearningCenter.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="https://netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/popgame.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script|Merienda" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
    <link href="~/Content/buttongo.css" rel="stylesheet" />
    <link href="~/Content/Gender.css" rel="stylesheet" />
    <link href="~/Content/AgeGroup1.css" rel="stylesheet" />
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" />
</head>
<body style="background-color:#90b4b4">
    <nav class="navbar navbar-default navbar-inverse" style="font-family: 'Merienda', cursive;background-color:#333">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Melbourne Myhome</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img src="~/Content/title.png" style="margin-top:2vh;margin-left:5vh" />
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav " style="margin-top:6px;margin-left:64px">
                    <li><a href="~/Home/Home"><img src="~/Content/homeicon.png" />Home <span class="sr-only">(current)</span></a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle;" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"> <img src="~/Content/learningicon.png" />Learning <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Pop/LearningCenter"><i class="fa fa-book" aria-hidden="true"></i>Learning Center</a></li>
                            <li><a href="~/Pop/PopOverview"><i class="fa fa-user" aria-hidden="true"></i>Population</a></li>
                            <li><a href="~/Culture/CulOverview"><i class="fa fa-globe" aria-hidden="true"></i>Culture</a></li>

                            <li><a href="~/Pop/Environment"><i class="fa fa-tree" aria-hidden="true"></i>Environment</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/Content/quizicon.png" />Quiz <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Quiz/QuizCenter"><i class="fa fa-file-text-o" aria-hidden="true"></i>Quiz Center</a></li>
                            <li><a href="~/Quiz/QuizPop"><i class="fa fa-female" aria-hidden="true"></i>Population Quiz</a></li>
                            <li><a href="~/Quiz/QuizCul"><i class="fa fa-flag" aria-hidden="true"></i>Culture Quiz</a></li>

                            <li><a href="~/Quiz/QuizEnv"><i class="fa fa-bolt" aria-hidden="true"></i>Environment Quiz</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/Content/gameicon.png" />Game <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Game/GameCenter"><i class="fa fa-gamepad" aria-hidden="true"></i>Game Center</a></li>
                            <li><a href="~/Game/MemoryGame"><i class="fa fa-github-alt" aria-hidden="true"></i>Memory Game</a></li>
                            <li><a href="~/Game/Tangram"><i class="fa fa-hand-o-up" aria-hidden="true"></i>Tangram Game</a></li>
                            <li><a href="~/Game/Capital"><i class="fa fa-check-square-o" aria-hidden="true"></i>Answer Game</a></li>
                            <li><a href="~/Game/RecycleGame"><i class="fa fa-recycle" aria-hidden="true"></i>Recycle Game</a></li>
                        </ul>
                    </li>

                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>


    <center><img src="~/Content/halff.png" style="margin-top:5vh" /></center>
    <h1 style="text-align:center;color:white;font-size:48px">Gender Rate</h1>
    <div id="contenitore">
        <div class="left">

            <table>
                <caption>&nbsp;Gender &nbsp; &nbsp;&nbsp;Amount &nbsp;&nbsp;&nbsp; Rate</caption>
                <tbody>

                    <tr>
                        <td>Male</td>
                        <td>
                            @foreach (var item in Model)
                            {
                                if (item.Sex == "Male")
                                {
                                    @Html.DisplayFor(modelItem => item.Population)
                                }
                            }
                        </td>
                        <td>
                            49%
                        </td>
                        <td style="background-color:#003366">&nbsp;</td>
                    </tr>
                    <tr>
                        <td>Female</td>
                        <td>
                            @foreach (var item in Model)
                            {
                                if (item.Sex == "Female")
                                {
                                    @Html.DisplayFor(modelItem => item.Population)
                                }

                            }
                        </td>
                        <td>
                            51%
                        </td>
                        <td style="background-color:#ff6600">&nbsp;</td>
                    </tr>

                </tbody>
            </table>

        </div>
        <div class="left">
            <div id="grafico">
                <div class="riga" style="top:25%"><div style="color:black">3,000,000</div></div>
                <div class="riga" style="top:50%"><div style="color:black">2,000,000</div></div>
                <div class="riga" style="top:75%"><div style="color:black">1,000,000</div></div>
                <div class="riga" style="top:5%"><div style="color:black">Population</div></div>

                <div id="col0" style="background-image: url('../../Content/male.png');left: 25%;top: 46%;color:black" class="column">
                </div>
                <div id="col1" style="background-image: url('../../Content/woman.png'); left: 50%; top: 42%;color:black; background-color: #ff6600;" class="column"> </div>

            </div>
        </div>
        <div class="canc"></div>

    </div>
    <center>
        <img src="~/Content/POP3.png" />
    </center>
    <center>
        <img src="~/Content/POP4.png" />
    </center>
    <h1 style="display:inline-block;margin-left:10%;color:black">2016 Age Group</h1>

    <center><img src="~/Content/agegroup1.png" /></center>

    <div>
        <a href="~/Population_Melbourne_Trendline/index" class="button" style="float:left">Previous</a>
        <a href="~/Population_Suburb/index" class="button" style="float:right">Next</a>
    </div>








    <script src="~/Scripts/Gender.js"></script>


</body>